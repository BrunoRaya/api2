datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Professional {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  email       String   @unique
  name        String
  profession  String
  address     Address
  cep         String
  complemento String
  cpf         String   @unique
  password    String   @unique

  @@map("Professional")
}

type Address {
  street String
  city   String
  state  String
}
